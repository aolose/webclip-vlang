[Unit]
Description=WebClip
After=network.target

[Service]
ExecStart=/home/webclip-vlang/webclip-vlang -crt=/root/tls/ivi.cx/ivi.cx.crt -key=/root/tls/ivi.cx/ivi.cx.key -ca=/root/chain.pem -port=7002
TimeoutStopSec=5s
LimitNOFILE=1048576
LimitNPROC=512
PrivateTmp=true
ProtectSystem=full
AmbientCapabilities=CAP_NET_BIND_SERVICE
Restart=always
RestartSec=120


[Install]
WantedBy=multi-user.target